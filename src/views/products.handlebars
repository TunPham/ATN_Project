<div class = 'container'>
    {{#each results}}
    <div class="card" style="width: 18rem;">
    <a href="/products/productsDetail?slug={{this.slug}}"><img class="card-img-top" src='uploads/{{this.image}}' alt="Card image cap"/></a>
        <div class="card-body">
            <h5 class="card-title">{{this.name}}</h5>
            <p class="card-text">{{this.price}} $</p>
            <p class="card-text">{{this.description}}</p>
            <a href="#" class="btn btn-primary">Add to cart</a>
        </div>
    </div>
    {{/each}}
</div>